{
  "name": "btcpayserver-ui-prototype",
  "description": "Prototype for a new BTCPayServer UI",
  "version": "0.1.0",
  "homepage": "https://github.com/dennisreimann/btcpayserver-ui-prototype",
  "bugs": "https://github.com/dennisreimann/btcpayserver-ui-prototype/issues",
  "repository": "dennisreimann/btcpayserver-ui-prototype",
  "scripts": {
    "start": "npm-run-all -p start:*",
    "start:patternlib": "uiengine build --serve --watch",
    "start:build": "npm-run-all -p build:*",
    "build:bootstrap": "node-sass --watch --recursive src/styles/index.scss src/static/bootstrap/bootstrap_build.css",
    "build:variables": "node-sass --watch --recursive src/styles/root.scss src/static/custom/btcpay-theme.css"
  },
  "keywords": [
    "BTCPayServer"
  ],
  "author": "Dennis Reimann <mail@dennisreimann.de>",
  "license": "MIT",
  "devDependencies": {
    "@uiengine/adapter-html": "2.3.1",
    "@uiengine/core": "2.5.7",
    "bootstrap": "4.3.1",
    "jquery": "3.4.1",
    "node-sass": "4.12.0",
    "npm-run-all": "4.1.5",
    "popper.js": "1.16.0",
    "rimraf": "3.0.0"
  }
}
